{
    "manifest_version": 3,
    "name": "Google Trends Word Cloud",
    "version": "1.0",
    "description": "Replaces the New Tab page with a real-time Google Trends word cloud.",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "http://localhost:3000/*",
        "https://*.railway.app/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}